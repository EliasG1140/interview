{
  "name": "back-addin",
  "version": "1.0.0",
  "author": "Elias Garcia",
  "main": "dist/index.js",
  "scripts": {
    "init": "npx prisma generate && npx prisma db push && npx prisma db seed",
    "clean": "rm -rf dist",
    "build": "npm run clean && npm run tsc",
    "tsc": "tsc",
    "start": "node .",
    "production": "npm run build && npm run start",
    "dev": "nodemon --watch src --ext ts --exec ts-node src/index.ts"
  },
  "dependencies": {
    "@prisma/client": "^4.7.1",
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-validator": "^6.14.2",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/cors": "^2.8.13",
    "@types/dotenv": "^8.2.0",
    "@types/express": "^4.17.15",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/module-alias": "^2.0.1",
    "@types/morgan": "^1.9.3",
    "@types/node": "^18.11.15",
    "nodemon": "^2.0.20",
    "prisma": "^4.7.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.4"
  },
  "prisma": {
    "seed": "ts-node --compiler-options {\"module\":\"CommonJS\"} prisma/seed.ts"
  }
}
